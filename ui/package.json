{
  "name": "api-insights-ui",
  "version": "0.1.0",
  "private": true,
  "title": "API Insights",
  "homepage": ".",
    "options": {
      "allowedHosts": [
        "localhost",
        ".localhost",
        "0.0.0.0",
        "backend"
      ]
    },
    "proxy": "http://backend:8081/",
  "scripts": {
    "prepare": "cd .. && husky install && npx husky set .husky/pre-commit \"cd ui && npm run pre-commit\"",
    "start": "HTTPS=true HOST=local.cisco.com PORT=3000 react-scripts start",
    "dockerstart": "HTTPS=false HOST=0.0.0.0 PORT=3000 react-scripts start",
    "build": "rm -rf dist && react-scripts build && mv build dist && mv package-dist.json ./dist/package.json",
    "build:docker": "rm -rf dist && react-scripts build && mkdir -p dist && mkdir  dist/api-insights-demo && mv -v build/* dist/api-insights-demo/ && mv package-dist.json ./dist/package.json",
    "build:staging": "DEVNET_BUILD_ENV=staging yarn build",
    "build:production": "DEVNET_BUILD_ENV=production yarn build",
    "test": "react-scripts test --coverage",
    "test:full": "react-scripts test --coverage --watchAll",
    "eject": "react-scripts eject",
    "lint": "eslint src",
    "lint:quiet": "eslint src --quiet",
    "lint:fix": "eslint src --fix",
    "pre-commit": "npm run lint:quiet && CI=true npm run test"
  },
  "dependencies": {
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@monaco-editor/react": "^4.3.1",
    "@mui/icons-material": "^5.5.1",
    "@mui/material": "^5.5.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/dompurify": "^2.3.3",
    "@types/jest": "^27.5.2",
    "@types/marked": "^4.0.3",
    "@types/node": "^16.11.59",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "@types/react-js-pagination": "^3.0.4",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "cross-var": "^1.1.0",
    "dayjs": "^1.11.0",
    "dompurify": "^2.3.6",
    "express": "^4.18.0",
    "http-server": "^14.1.0",
    "marked": "^4.0.14",
    "monaco-editor": "^0.34.0",
    "node-sass": "^7.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.2.2",
    "react-scripts": "5.0.1",
    "react-query": "^3.34.19",
    "react-js-pagination": "^3.0.3",
    "react-monaco-editor": "^0.50.1",
    "serve": "^13.0.2",
    "typescript": "^4.8.3",
    "web-vitals": "^2.1.0",
    "yaml": "^2.1.1",
    "eslint": "^8.23.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
